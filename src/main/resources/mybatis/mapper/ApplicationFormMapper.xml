<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xp.graduation.mapper.ApplicationFormMapper">
    <!--用户报名-->
    <insert id="insertInfo" useGeneratedKeys="true" keyProperty="id">
        insert into applicationform(username,pname,companyname,cost,teacher) values(#{username},#{trainingSchedule.pname},#{trainingSchedule.companyname},#{trainingSchedule.cost},#{teacher})
    </insert>

</mapper>